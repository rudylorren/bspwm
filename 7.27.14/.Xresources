URxvt*buffered: on
URxvt*geometry: 75x20
Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

!URxvt*font:-*-gohufont-medium-r-normal-*-14-*-*-*-*-*-*-*
!URxvt*boldItalicFont:-*-gohufont-medium-r-normal-*-14-*-*-*-*-*-*-*
URxvt*font:-*-gohufont-medium-r-normal-*-12-80-100-100-*-*-*-*
URxvt*boldItalicFont:-*-gohufont-medium-r-normal-*-12-80-100-100-*-*-*-*
!URxvt*font:-*-terminus-medium-r-normal-*-14-*-*-*-*-*-*-*
!URxvt*boldItalicFont:- *-terminus-medium-r-normal-*-14-*-*-*-*-*-*-*
!URxvt*font: -*-termsyn-medium-*-normal-*-14-*-*-*-*-*-*-*
!URxvt*boldItalicFont: -*-termsyn-medium-*-normal-*-14-*-*-*-*-*-*-* 
!URxvt*font:xft:termsyn:regular:size=8 
!URxvt*boldItalicFont:xft:termsyn:regular:size=8
!URxvt*font: xft:Fira Mono OT:regular:size=8
!URxvt*boldItalicFont:Fira Mono OT:regular:size=8
!URxvt*font: -*-tamsyn-medium-*-*-*-15-*-*-*-*-*-*-*
!URxvt*boldItalicFont: -*-tamsyn-medium-*-*-*-15-*-*-*-*-*-*-*
!URxvt*font:-*-dina-medium-r-*-*-10-*-*-*-*-*-*-*
!URxvt*boldItalicFont:-*-dina-medium-r-*-*-10-*-*-*-*-*-*-*
URxvt*scrollBar: False
URxvt*saveLines: 99999
URxvt*pointerBlank: True
!URxvt.letterSpace: -1
URxvt*colorn: #76844A
URxvt*perl-ext-common: default,url-select,clipboard,matcher
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.url-select.launcher: /usr/bin/firefox -new-tab 
URxvt.url-select.underline: true
URxvt.keysym.M-Escape:perl:keyboard-select:activate
URxvt.keysym.M-c:     perl:clipboard:copy
URxvt.keysym.M-v:     perl:clipboard:paste
URxvt.keysym.M-C-v:   perl:clipboard:paste_escaped

*background: #111111
*foreground: #c4c4c4

! Black (not tango) + DarkGrey
*color0:  #090909
*color8:  #2d2d2d
! DarkRed + Red
*color1:  #6f2522
*color9:  #9e2c26
! DarkGreen + Green
*color2:  #3c472b
*color10: #597630
! DarkYellow + Yellow
*color3:  #62542b
*color11: #b79326
! DarkBlue + Blue
*color4:  #2f3146
*color12: #454975
! DarkMagenta + Magenta
*color5:  #352646
*color13: #5e4679
!DarkCyan + Cyan (both not tango)
*color6:  #263b46
*color14: #487084
! LightGrey + White
*color7:  #777777
*color15: #cccccc


